{
  "name": "@kuyo/sdk",
  "version": "1.0.0",
  "description": "Kuyo SDK - Performance monitoring and error tracking",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "size": "npm run build && npx bundle-size",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "performance",
    "monitoring",
    "error-tracking",
    "analytics",
    "sdk",
    "web-vitals",
    "nextjs"
  ],
  "author": "Antonin Marchard",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "hono-rate-limiter": "0.4.2",
    "tslib": "2.8.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/kuyo-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/kuyo-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/kuyo-sdk#readme"
}
